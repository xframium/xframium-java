<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
  <model>
    
    <page name="home" />

  </model>

  <function name="kw-checkcontrast" active="true">

    <step type="VERIFY_CONTRAST" name="alertButton" page="home">
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="7" />
      <parameter type="STATIC" value="21" />
    </step>
    
    <step type="VERIFY_CONTRAST" name="colorContrastAA" page="home" inverse="true">
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="7" />
      <parameter type="STATIC" value="21" />
    </step>
    
    <step type="VERIFY_CONTRAST" name="colorContrastAA" page="home">
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="4.5" />
      <parameter type="STATIC" value="7" />
    </step>
    
    <step type="VERIFY_CONTRAST" name="colorContrast" page="home" inverse="true">
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="4.5" />
      <parameter type="STATIC" value="21" />
    </step>
    


  </function>

</suite>
