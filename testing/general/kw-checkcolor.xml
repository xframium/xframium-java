<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
  <model>
    
    <page name="home" />

  </model>

  <function name="kw-checkcolor" active="true">

    <step type="VERIFY_COLOR" name="alertButton" page="home">
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="5,5" />
    </step>
    
    <step type="VERIFY_COLOR" name="colorCheck" page="home" >
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="5,5" />
      <parameter type="STATIC" value="#337ab7" />
      <parameter type="STATIC" value="0" />
    </step>
    
    <step type="VERIFY_COLOR" name="colorCheck" page="home" context="alertColor">
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="5,5" />
    </step>

    <step type="COMPARE" name="checkColor" page="home">
      <parameter type="CONTEXT" value="alertColor" />
      <parameter type="STATIC" value="-13403465" />
    </step>
    
    <step type="VERIFY_COLOR" name="colorCheck" page="home" >
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="5,5" />
      <parameter type="STATIC" value="51,122,183" />
      <parameter type="STATIC" value="5" />
    </step>
    
    <step type="VERIFY_COLOR" name="colorCheck" page="home" >
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="5,5" />
      <parameter type="STATIC" value="-13403465" />
      <parameter type="STATIC" value="5" />
    </step>
    
    <step type="VERIFY_COLOR" name="colorCheck" page="home" inverse="true" >
      <parameter type="STATIC" value="high" />
      <parameter type="STATIC" value="5,5" />
      <parameter type="STATIC" value="#000000" />
      <parameter type="STATIC" value="0" />
    </step>
    


  </function>

</suite>
